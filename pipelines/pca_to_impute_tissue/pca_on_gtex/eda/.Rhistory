setwd('work/pca_on_gtex/eda/')
df <- read.table('norm_avgs', stringsAsFactors = FALSE)
head(df)
colnames(df) <- "norm_means"
library(ggplot2)
ggplot(df, aes(norm_means)) + geom_density()
